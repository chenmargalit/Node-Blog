<% include ../partials/header %>
<div class="container">

    <header class="jumbotron">

</header>

<h1>edit <%= att.name %></h1>


<form action="/att/<%= att._id %>?_method=PUT" method="POST">
      <div class="form-group">

    <label>Name</label>
    <input type="text" name="Att[name]" class="form-control" id="exampleInputEmail1" value="<%= att.name %>">
    </div>
 <div class="form-group">

    <label>Image</label>
    <input type="text" name="Att[image]" class="form-control" id="exampleInputEmail1" value="<%= att.image %>">
 </div>
 <div class="form-group">
     
 <label>How far?</label>
    <input type="number" name="Att[time]" class="form-control" placeholder="time" value="<%=att.time %>" required>
</div>
  <div class="form-group">
    <label>text</label>
        <input type="text" name="Att[text]" class="form-control" id="exampleInputEmail1" value="<%= att.text %>">
    </div>
    

    <button type="submit" class="btn btn-success">Submit</button>

</form>

</div>

<% include ../partials/footer %>



